<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScalingAnimationDroid">
        <attr name="durationActionDown" format="integer" />
        <attr name="durationActionUp" format="integer" />
        <attr name="scalingAnimationType" format="enum">
            <enum name="scaling_in" value="0" />
            <enum name="scaling_out" value="1" />
